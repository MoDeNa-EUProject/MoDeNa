	journal = {Computational Materials Science},
	keywords = {[Physical Astronomy Classification Scheme] 7.05.T; [Physical Astronomy Classification Scheme] 82.20.Wt; [Physical Astronomy Classification Scheme] 81.40.-z; Simulation; Microstructure; RCP-LV diagram; Voronoi diagram; Possion-Voronoi diagram; Volume distribution; Face number distribution; Grain growth},
	language = {English},
