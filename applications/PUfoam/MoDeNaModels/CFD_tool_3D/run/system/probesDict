FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Fields to be probed. runTime modifiable!
        fields
        (   
            CE_I0
            CE_I1
            CE_I2
            CE_A0
            CE_A1
            CE_Areac0
            CE_Areac1
            CE_B
            CE_B2
        );


// Locations to be probed. runTime modifiable!
        probeLocations
        (
            ( 0.005 0.005 0.005 ) // P1
           // ( 0.05 0.05 0.01 )  // P2
            //( 0.05 0.1 0.01 )   // P3
            //( 0.05 0.15 0.01 )   // P4
        );


