FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      wCO2_g.org;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0; 

boundaryField
{
   Wall       
    {
        type            zeroGradient;
    }

    atmosphere      
    {
	type		zeroGradient;
    }

axis1
	{
		type    symmetryPlane;
	}
axis2
	{
		type    symmetryPlane;
	}
    frontAndBack    
    {
        type            empty;
    }
}


